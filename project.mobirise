          "_customHTML": "<section class=\"coworkingamp-footer1 footer1\" group=\"Footers\" once=\"footer\" plugins=\"amp-main, amp-form, amp-mustache\" global always-bottom>\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <header>Logo</header>\n        <select name=\"showLogo\" title=\"Logo\">\n            <option value=\"1\">Logo</option>\n            <option value=\"2\" selected>Brand</option>\n        </select>\n        <input type=\"range\" inline title=\"Logo Size\" name=\"logoSize\" min=\"1\" max=\"10\" step=\"1\" value=\"3\" condition=\"showLogo == 1\">\n        <header>Social</header>\n        <input type=\"checkbox\" title=\"Social\" name=\"showSocial\">\n        <input type=\"range\" inline title=\"Icons Count\" name=\"iconsCount\" min=\"1\" max=\"5\" value=\"3\" step=\"1\" condition=\"showSocial\">\n        <input type=\"color\" title=\"Color\" value=\"#282828\" name=\"iconColor\" condition=\"showSocial\">\n        <input type=\"range\" inline title=\"Icons Size\" name=\"iconsSize\" min=\"10\" max=\"20\" value=\"12\" step=\"1\" condition=\"showSocial\">\n        <header>Form</header>\n        <input type=\"checkbox\" title=\"Form\" name=\"showForm\" checked>\n        <input type=\"color\" title=\"Color\" value=\"#cccccc\" name=\"formTextColor\" condition=\"showForm\">\n        <input type=\"color\" title=\"Border\" value=\"#616161\" name=\"formBorder\" condition=\"showForm\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\">\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopyright\" checked>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"range\" inline title=\"Menu Item\" name=\"menuItem\" min=\"1\" max=\"10\" value=\"6\" step=\"1\">\n        <input type=\"checkbox\" title=\"Adress\" name=\"showAdress\" checked>\n        <input type=\"checkbox\" title=\"Phone\" name=\"showPhone\" checked>\n        <input type=\"checkbox\" title=\"Mail\" name=\"showMail\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/coworkingamp/components/_images/background3.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#1c1c1c\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n    <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type == 'image'\">\n    <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n\n<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    \n    <div class mbr-class=\"{'container' :  !fullWidth, 'container-fluid' : fullWidth}\">\n        <div class=\"mbr-row\">\n            <div class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-5\">\n                <div class=\"wrap_left\">\n                    <div class=\"logo_wrap mbr-pb-3\">\n                        <div class=\"image-block\" mbr-if=\"showLogo == 1\">\n                            <amp-img src=\"@PROJECT_PATH@/assets/images/logo1.png\" layout=\"responsive\" width=\"16\" height=\"9\" alt=\"Mobirise\" class=\"mobirise-loader\">\n                                <div placeholder class=\"placeholder\"></div>\n                                <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/logo1.png\" alt=\"Mobirise\">\n                            </amp-img>\n                        </div>\n                        <div class=\"brand\" mbr-if=\"showLogo == 2\">\n                            <h3 class=\"mbr-fonts-style brand_name mbr-white mbr-semibold\" mbr-theme-style=\"display-5\">Royal Garden</h3>\n                        </div>\n                    </div>\n                    <p class=\"mbr-fonts-style mbr-text info mbr-pb-2\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".info\">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Eum, sunt rem dolorum necessitatibus maxime unde.</p>\n                    <div class=\"mbr-pb-2 align-center\" mbr-if=\"showSocial\">\n                    <!-- SOCIAL ICON -->\n                        <div class=\"social_list mbr-flex flex-wrap\" mbr-list mbr-list-grow=\"iconsCount\" mbr-link-type=\"socials\">\n                            <a href=\"#\" target=\"_blank\">\n                                <span mbr-icon class=\"amp-iconfont fa-facebook-f fa\"></span>\n                            </a>\n                            <a href=\"#\" target=\"_blank\">\n                                <span mbr-icon class=\"amp-iconfont fa-twitter fa\"></span>\n                            </a>\n                            <a href=\"#\" target=\"_blank\">\n                                <span mbr-icon class=\"amp-iconfont fa-pinterest-p fa\"></span>\n                            </a>\n                        </div>\n                    <!-- SOCIAL ICON END -->\n                    </div>\n\n                    <div class=\"mbr-form\" mbr-form mbr-if=\"showForm\">\n    <!--Formbuilder Form-->\n    <form action name=\"Mobirise Form\" method=\"POST\" class=\"mbr-form align-center\">\n    <div class=\"mbr-row\">\n    <div submit-success class=\"mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n    <template data-form-alert type=\"amp-mustache\">Subscription successful!</template>\n    </div>\n    <div submit-error class=\"mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n    <template data-form-alert type=\"amp-mustache\">Subscription successful!</template>\n    </div>\n    </div>\n    <div class=\"dragArea mbr-row\">\n    <div data-for=\"form[data][0][1]\" class=\"mbr-col field\">\n    <input type=\"hidden\" name=\"form[data][0][0]\" value=\"Email\">\n    <input type=\"email\" name=\"form[data][0][1]\" placeholder=\"Subscribe\" data-form-field=\"Email\" mbr-theme-style=\"display-4\" required=\"required\" class=\"field-input\" value>\n    </div>\n    <div mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" class=\"mbr-col-auto mbr-section-btn field\"><a type=\"submit\" class=\"btn btn-sm btn-form btn-black\" data-app-placeholder=\"Type Text\">Go<span class=\"mobi-mbri mobi-mbri-arrow-next mbr-iconfont mbr-iconfont-btn\"></span></a></div>\n    </div>\n    </form><!--Formbuilder Form-->\n    </div>\n                    <div>\n                        <p class=\"copyright mbr-fonts-style mbr-pt-1\" mbr-if=\"showCopyright\" mbr-theme-style=\"display-4\" data-app-selector=\".copyright\" data-app-placeholder=\"Type Text\"><a href=\"#\" class=\"text-white\"> Copyright 2025 JSK's Sofa - All Rights Reserved</a></p>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-7\">\n                <div class=\"mbr-row\">\n                    <div class=\"mbr-col-lg-6 mbr-col-md-6 mbr-col-sm-12\">\n                        <div class=\"about_wrap\">\n                            <h4 class=\"mbr-fonts-style mbr-white mbr-section-title group-title mbr-pb-4 mbr-semibold\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".group-title\">About</h4>\n                            <p class=\"about mbr-text mbr-pb-1 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".about\">Welcome to our Sofa and Furniture website, where you will find an extensive collection of premium quality sofas and furniture pieces to elevate the look of your home. We take pride in offering a wide range of styles, sizes, colors, and materials to cater to every taste and budget.</p>\n                            <div class=\"mbr-flex contacts mbr-pt-5\" mbr-if=\"showAdress\">\n                                <span class=\"mbr-iconfont mobi-mbri-map-pin mobi-mbri\" mbr-icon></span>\n                                <p class=\"mbr-fonts-style adress mbr-text mbr-semibold mbr-white\" mbr-theme-style=\"display-4\" data-app-selector=\".adress\"><a href=\"#\" class=\"text-white\">Plot No.204 A, Odhan - Dabwali Road, Opposite Royal Market, Sirsa Haryana 12507</a>7</p>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"mbr-col-lg-6 mbr-col-md-6 mbr-col-sm-12\">\n                        <h4 class=\"mbr-fonts-style mbr-white mbr-section-title group-title mbr-pb-4 mbr-semibold\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".group-title\">Quick Links</h4>\n                        <div class=\"items mbr-pb-5\" mbr-list mbr-list-grow=\"menuItem\">\n                            <p class=\"item mbr-pb-1 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item, .items\">\n                                    <a href=\"index.html\" class=\"text-white text-primary\">- Home</a></p>\n                            <p class=\"item mbr-pb-1 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item, .items\">\n                                    <a href=\"AboutUs.html\" class=\"text-white text-primary\">- About Us</a></p>\n                            <p class=\"item mbr-pb-1 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item, .items\">\n                                    <a href=\"Room.html\" class=\"text-white text-primary\">- S</a>ofa</p>\n                            \n\n                        <p class=\"item mbr-pb-1 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item, .items\">\n                                    <a href=\"Banquets.html\" class=\"text-white text-primary\">- B</a>edroom</p><p class=\"item mbr-pb-1 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item, .items\">\n                                    <a href=\"Dining.html\" class=\"text-white text-primary\">- Dining</a></p><p class=\"item mbr-pb-1 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item, .items\">\n                                    <a href=\"gallery.html\" class=\"text-white text-primary\">- Gallery</a></p></div>\n                        \n                        <div class=\"mbr-flex contacts\" mbr-if=\"showPhone\">\n                            <span class=\"mbr-iconfont mobi-mbri-phone mobi-mbri\" mbr-icon></span>\n                            <p class=\"mbr-fonts-style phone mbr-text mbr-semibold mbr-white\" mbr-theme-style=\"display-4\" data-app-selector=\".phone\"><a href=\"#\" class=\"text-white\">+91 1</a>2345-67894</p>\n                        </div>\n                        \n                        <div class=\"mbr-flex contacts\" mbr-if=\"showMail\">\n                            <span class=\"mbr-iconfont mobi-mbri-letter mobi-mbri\" mbr-icon></span>\n                            <p class=\"mbr-fonts-style mail mbr-text mbr-semibold mbr-white\" mbr-theme-style=\"display-4\" data-app-selector=\".mail\"><a href=\"#\" class=\"text-white\">GroupSixmcgncsirsa22-23@gmail</a></p>\n                        </div>\n                        \n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
